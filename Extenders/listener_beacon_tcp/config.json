{
  "extender_type": "listener",
  "extender_file": "listener_beacon_tcp.so",

  "listener_name": "BeaconTCP",
  "listener_type": "internal",
  "protocol": "bind_tcp",

  "ui": {
    "layout": "glayout",
    "elements": [
      {
        "type": "vspacer",
        "position": [ 0, 0, 1, 2 ]
      },
      {
        "type": "label",
        "text": "Bind port:",
        "position": [ 1, 0, 1, 1 ]
      },
      {
        "type": "spinbox",
        "id": "port_bind",
        "editable": "false",
        "min": 1,
        "max": 65535,
        "value": 9000,
        "position": [1, 1, 1, 1]
      },
      {
        "type": "label",
        "text": "Prepend data:",
        "position": [2, 0, 1, 1]
      },
      {
        "type": "input",
        "id": "prepend_data",
        "editable": "false",
        "placeholder": "\\x12\\xabSimple\\x20word\\xa",
        "position": [2, 1, 1, 1]
      },
      {
        "type": "vspacer",
        "position": [ 3, 0, 1, 2 ]
      }
    ]
  }
}