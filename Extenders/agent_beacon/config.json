{
  "extender_type": "agent",
  "extender_file": "agent_beacon.so",

  "agent_name": "beacon",
  "agent_watermark": "be4c0149",

  "listeners": [
    {
      "listener_name": "BeaconHTTP",
      "configs" : [
        {
          "operating_system": "windows",
          "handler": "handler_external",
          "generate_ui": {
            "layout": "glayout",
            "elements": [
              {
                "type": "label",
                "text": "Arch:",
                "position": [0, 0, 1, 1]
              },
              {
                "type": "combo",
                "items": ["x64", "x86"],
                "id": "arch",
                "position": [0, 1, 1, 2]
              },
              {
                "type": "label",
                "text": "Format:",
                "position": [1, 0, 1, 1]
              },
              {
                "type": "combo",
                "items": ["Exe", "Service Exe", "DLL", "Shellcode" ],
                "id": "format",
                "position": [1, 1, 1, 2]
              },
              {
                "type": "hline",
                "position": [2, 0, 1, 3]
              },
              {
                "type": "label",
                "text": "Sleep (Jitter %):",
                "position": [3, 0, 1, 1]
              },
              {
                "type": "input",
                "id": "sleep",
                "placeholder": "1h 2m 5s",
                "text": "4s",
                "position": [3, 1, 1, 1]
              },
              {
                "type": "spinbox",
                "id": "jitter",
                "min": 0,
                "max": 100,
                "position": [3, 2, 1, 1]
              },
              {
                "type": "label",
                "text": "Service Name:",
                "position": [4, 0, 1, 1]
              },
              {
                "type": "input",
                "id": "svcname",
                "text": "AgentService",
                "position": [4, 1, 1, 2]
              }
            ]
          }
        }
      ]
    },
    {
