{
  "extender_type": "listener",
  "extender_file": "listener_gopher_tcp.so",

  "listener_name": "GopherTCP",
  "listener_type": "external",
  "protocol": "tcp",

  "ui": {
    "layout": "glayout",
    "elements": [
      {
        "type": "label",
        "text": "Host & port (Bind):",
        "position": [ 0, 0, 1, 1 ]
      },
      {
        "type": "input",
        "id": "host_bind",
        "editable": "false",
        "position": [ 0, 1, 1, 2 ]
      },
      {
        "type": "spinbox",
        "id": "port_bind",
        "editable": "false",
        "min": 1,
        "max": 65535,
        "value": 4444,
        "position": [0, 3, 1, 1]
      },
      {
        "type": "label",
        "text": "Callback addresses:",
        "position": [ 1, 0, 1, 1 ]
      },
      {
        "type": "textedit",
        "id": "callback_addresses",
        "placeholder": "192.168.1.1:4444\nserver2.com:5555",
        "position": [ 1, 1, 1, 3 ]
      },
      {
        "type": "label",
        "text": "Timeout (sec):",
        "position": [ 2, 0, 1, 1 ]
      },
      {
        "type": "spinbox",
        "id": "timeout",
        "min": 1,
        "max": 1000000,
        "value": 10,
        "position": [2, 1, 1, 3]
      },
      {
        "type": "label",
        "text": "TCP banner:",
        "position": [ 3, 0, 1, 1 ]
      },
      {
        "type": "textedit",
        "id": "tcp_banner",
        "text": "AdaptixC2 server\n",
        "position": [ 3, 1, 1, 3 ]
      },
      {
        "type": "label",
        "text": "Error answer:",
        "position": [ 4, 0, 1, 1 ]
      },
      {
        "type": "textedit",
        "id": "error_answer",
        "text": "Connection error...\n",
        "position": [ 4, 1, 1, 3 ]
      },
      {
        "type": "checkbox",
        "text": "Use mTLS",
        "id": "ssl",
        "editable": "false",
        "position": [ 5, 0, 1, 2]
      },
      {
        "type": "file_selector",
        "id": "ca_cert",
        "placeholder": "CA cert",
        "editable": "false",
        "position": [ 5, 2, 1, 2 ]
      },
      {
        "type": "file_selector",
        "id": "server_cert",
        "placeholder": "Server cert",
        "editable": "false",
        "position": [ 6, 0, 1, 2 ]
      },
      {
        "type": "file_selector",
        "id": "server_key",
        "placeholder": "Server key",
        "editable": "false",
        "position": [ 6, 2, 1, 2 ]
      },

      {
        "type": "file_selector",
        "id": "client_cert",
        "placeholder": "Client cert",
        "editable": "false",
        "position": [ 7, 0, 1, 2 ]
      },
      {
        "type": "file_selector",
        "id": "client_key",
        "placeholder": "Client key",
        "editable": "false",
        "position": [ 7, 2, 1, 2 ]
      }
    ]
  }
}